      "enable_saml": true,
      "saml_port": 8021,
      "browser": "firefox",
      "browser_profile": "default-release",
      "auto_reconnect": true,
      "auto_reconnect_interval": 15,
      "routing_rules": ["10.10.0.0/16", "intranet.example.com"],
      "custom_dns": ["10.10.1.2", "10.10.1.3"],
      "persistent": true
    }
  ]
}
